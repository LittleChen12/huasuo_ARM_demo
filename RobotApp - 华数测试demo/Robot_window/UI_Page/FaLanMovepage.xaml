<Page x:Class="Robot_window.UI_Page.FaLanMovepage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Robot_window.UI_Page"
      mc:Ignorable="d"
      xmlns:RCL="clr-namespace:RobotControlLibrary;assembly=RobotControlLibrary"
      
      d:DesignHeight="380" d:DesignWidth="280"
      Title="FaLanMovepage">

    <Grid>
        <RCL:RobotMoveContorl Grid.Column="0"
                              Margin="59,34,0,0"
                              IntervalTime="30"
                              x:Name="X"
                              IncrementValue="2"
                              Clickd="JointValueChanged" />
        <RCL:RobotMoveContorl Grid.Column="0"
                              Margin="59,84,0,0"
                              IntervalTime="30"
                              x:Name="Y"
                              IncrementValue="2"
                              Clickd="JointValueChanged" />
        <RCL:RobotMoveContorl Grid.Column="0"
                              Margin="59,134,0,0"
                              IntervalTime="30"
                              x:Name="Z"
                              IncrementValue="2"
                              Clickd="JointValueChanged" />
        <RCL:RobotMoveContorl Grid.Column="0"
                              Margin="59,184,0,0"
                              IntervalTime="30"
                              x:Name="A"
                              IncrementValue="1"
                              Clickd="JointValueChanged" />
        <RCL:RobotMoveContorl Grid.Column="0"
                              Margin="59,234,0,0"
                              IntervalTime="30"
                              x:Name="B"
                              IncrementValue="1"
                              Clickd="JointValueChanged" />
        <RCL:RobotMoveContorl Grid.Column="0"
                              Margin="59,284,0,0"
                              IntervalTime="30"
                              x:Name="C"
                              IncrementValue="1"
                              Clickd="JointValueChanged" />


    </Grid>
</Page>
