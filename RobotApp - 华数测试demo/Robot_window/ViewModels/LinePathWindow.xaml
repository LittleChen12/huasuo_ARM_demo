<Window x:Class="Robot_window.ViewModels.LinePathWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Robot_window.ViewModels"
        mc:Ignorable="d"
        Title="LinePathWindow"
        Height="300"
        Width="400">
    <Grid>
        <Label x:Name="ob_title"
               Content="平面扫查点"
               HorizontalAlignment="Right"
               Margin="0,0,40,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontSize="13"
               Height="33"
               Width="204" />
        <Label x:Name="ob_x_location"
               Content="Start:X"
               HorizontalAlignment="Left"
               FontWeight="Bold"
               Margin="0,29,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontSize="10"
               Height="29"
               Width="45" />
        <Label x:Name="ob_y_location"
               Content="Y"
               HorizontalAlignment="Left"
               Margin="149,29,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               Height="24"
               Width="18" />
        <Label x:Name="ob_z_location"
               Content="Z"
               HorizontalAlignment="Right"
               Margin="0,29,94,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               Height="23"
               Width="18" />
        <TextBox x:Name="startx"
                 HorizontalAlignment="Left"
                 Text="500"
                 FontWeight="Bold"
                 Margin="47,31,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 Height="23"
                 Width="50" />
        <TextBox x:Name="starty"
                 HorizontalAlignment="Left"
                 Text="-20"
                 Margin="167,29,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="24"
                 Width="50" />
        <TextBox x:Name="startz"
                 Text="200"
                 HorizontalAlignment="Right"
                 Margin="0,29,42,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="23"
                 Width="50" />
        <Label x:Name="ob_xrot"
               Content="End:X"
               HorizontalAlignment="Left"
               FontWeight="Bold"
               Margin="-1,89,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontSize="10"
               Height="23"
               Width="42" />
        <Label x:Name="ob_yrot"
               Content="Y"
               HorizontalAlignment="Left"
               Margin="146,89,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="24"
               Width="30" />
        <Label x:Name="ob_zrot"
               Content="Z"
               HorizontalAlignment="Right"
               Margin="0,89,89,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="23"
               Width="19" />
        <TextBox x:Name="EndX"
                 Text="600"
                 HorizontalAlignment="Left"
                 FontWeight="Bold"
                 Margin="43,89,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 Height="23"
                 Width="50" />
        <TextBox x:Name="EndY"
                 HorizontalAlignment="Left"
                 Text="-20"
                 Margin="165,89,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="24"
                 Width="50" />
        <TextBox x:Name="EndZ"
                 HorizontalAlignment="Right"
                 Text="200"
                 Margin="0,89,37,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="23"
                 Width="50" />
        <Button x:Name="en"
                Content="确定"
                HorizontalAlignment="Left"
                Margin="93,221,0,0"
                VerticalAlignment="Top"
                Width="50"
                Height="28"
                FontSize="11"
                Click="en_Click" />
        <Button x:Name="not"
                Content="取消"
                Margin="216,221,134,0"
                VerticalAlignment="Top"
                Width="50"
                Height="28"
                FontSize="11"
                Click="not_Click" />
        <Label x:Name="ob_xrot复制__C_"
               Content="A_start"
               HorizontalAlignment="Left"
               FontWeight="Bold"
               Margin="-1,142,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontSize="10"
               Height="23"
               Width="42" />
        <Label x:Name="ob_yrot复制__C_"
               Content="B_start"
               HorizontalAlignment="Left"
               Margin="127,142,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="24"
               Width="45" />
        <Label x:Name="ob_zrot复制__C_"
               Content="Cstart"
               HorizontalAlignment="Left"
               Margin="272,142,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="23"
               Width="47" />
        <TextBox x:Name="Astart"
                 Text="180"
                 HorizontalAlignment="Left"
                 FontWeight="Bold"
                 Margin="43,142,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 Height="23"
                 Width="50" />
        <TextBox x:Name="Bstart"
                 Text="0"
                 HorizontalAlignment="Left"
                 Margin="165,142,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="24"
                 Width="50" />
        <TextBox x:Name="Cstart"
                 Text="90"
                 HorizontalAlignment="Left"
                 Margin="313,142,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="23"
                 Width="50" />
        <Label x:Name="ob_xrot复制__C_复制__C_"
               Content="A_end"
               HorizontalAlignment="Left"
               FontWeight="Bold"
               Margin="-1,188,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontSize="10"
               Height="23"
               Width="42" />
        <Label x:Name="ob_yrot复制__C_复制__C_"
               Content="Bend"
               HorizontalAlignment="Left"
               Margin="119,0,0,73"
               VerticalAlignment="Bottom"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="24"
               Width="46" />
        <Label x:Name="ob_zrot复制__C_复制__C_"
               Content="Cend"
               HorizontalAlignment="Left"
               Margin="266,187,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="23"
               Width="47" />
        <TextBox x:Name="Aend"
                 Text="180"
                 HorizontalAlignment="Left"
                 FontWeight="Bold"
                 Margin="43,188,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 Height="23"
                 Width="50" />
        <TextBox x:Name="Bend"
                 Text="0"
                 HorizontalAlignment="Left"
                 Margin="165,0,0,73"
                 VerticalAlignment="Bottom"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="24"
                 Width="50" />
        <TextBox x:Name="Cend"
                 HorizontalAlignment="Left"
                 Text="90"
                 Margin="313,188,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="23"
                 Width="50" />
        <TextBox x:Name="_num"
                 HorizontalAlignment="Left"
                 Text="20"
                 Margin="313,220,0,0"
                 VerticalAlignment="Top"
                 Foreground="Black"
                 FontWeight="Bold"
                 Height="23"
                 Width="50" />
        <Label x:Name="nummm"
               Content="num"
               HorizontalAlignment="Left"
               Margin="266,220,0,0"
               VerticalAlignment="Top"
               Foreground="Black"
               FontWeight="Bold"
               FontSize="10"
               Height="23"
               Width="47" />


    </Grid>
</Window>
