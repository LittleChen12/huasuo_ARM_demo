<Window x:Class="Robot_window.ViewModels.SpeedSetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Robot_window.ViewModels"
        mc:Ignorable="d"
        Title="SpeedSetWindow"
        Height="350"
        Width="400">
    <Grid Margin="0,0,-19,0">
        <Label Content="JointSpeed"
               Margin="20"
               Width="80"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="jointspeed"
                 Text="1"
                 Width="40"
                 Height="25"
                 Margin="100,20"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top" />
        <Label Content="XYZSpeed"
               Margin="20,60,0,0"
               Width="80"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="XYZspeed"
                 Text="1"
                 Width="40"
                 Height="25"
                 Margin="100,60"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top" />
        <Label Content="ABCSpeed"
               Margin="20,100,0,0"
               Width="80"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="ABCspeed"
                 Text="1"
                 Width="40"
                 Height="25"
                 Margin="100,100"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top" />
        <Label Content="2183Speed"
               Margin="20,141,0,0"
               Width="80"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="c2183speed"
                 Text="100"
                 Width=" 40"
                 Height=" 25"
                 HorizontalAlignment="Left"
                 VerticalAlignment="Top"
                 Margin="100,140" />
        <Label Content="JointInterlValue"
               Margin="156,20,0,0"
               Width="100"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="jointintelvalue"
                 Text="1"
                 Width="40"
                 Height="25"
                 Margin="0,20,125,0"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top" />
        <Label Content="XYZInterlValue"
               Margin="156,60,0,0"
               Width="100"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="XYZintelvalue"
                 Text="2"
                 Width="40"
                 Height="25"
                 Margin="0,59,125,0"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top" />
        <Label Content="ABCInterlValue"
               Margin="156,100,0,0"
               Width="100"
               Height="25"
               VerticalAlignment="Top"
               HorizontalAlignment="Left" />
        <TextBox x:Name="ABCintelvalue"
                 Text="1"
                 Width="40"
                 Height="25"
                 Margin="0,101,125,0"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Top" />
        <Label Content="tick"
               HorizontalAlignment="Right"
               VerticalAlignment="Bottom"
               Margin="0,0,210,167" />
        <TextBox x:Name="Tick"
                 HorizontalAlignment="Right"
                 VerticalAlignment="Bottom"
                 Text="10"
                 Width="40"
                 Height="25"
                 Margin="0,0,127,169" />


        <Button Content="确定"
                Height="30"
                Width="50"
                Margin="54,193,165,42"
                Click="SpeedSetClick" />

    </Grid>
</Window>
