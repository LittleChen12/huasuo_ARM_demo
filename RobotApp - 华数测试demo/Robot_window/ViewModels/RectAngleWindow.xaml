<Window x:Class="Robot_window.ViewModels.RectAngleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Robot_window.ViewModels"
        mc:Ignorable="d"
        Title="RectAngleWindow" Height="300" Width="400">
    <Viewbox>
        <Grid>
            <Label x:Name="ob_title"
                   Content="方形扫查试件"
                   HorizontalAlignment="Right"
                   Margin="0,0,40,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontSize="13"
                   Height="33"
                   Width="204" />
            <Label x:Name="ob_x_location"
                   Content="Center:X"
                   HorizontalAlignment="Left"
                   FontWeight="Bold"
                   Margin="0,29,0,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontSize="10"
                   Height="29"
                   Width="60" />
            <Label x:Name="ob_y_location"
                   Content="Y"
                   HorizontalAlignment="Left"
                   Margin="149,29,0,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontWeight="Bold"
                   Height="24"
                   Width="18" />
            <Label x:Name="ob_z_location"
                   Content="Z"
                   HorizontalAlignment="Right"
                   Margin="0,29,94,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontWeight="Bold"
                   Height="23"
                   Width="18" />
            <TextBox x:Name="centerx"
                     HorizontalAlignment="Left"
                     Text="450"
                     FontWeight="Bold"
                     Margin="52,31,0,0"
                     VerticalAlignment="Top"
                     Foreground="Black"
                     Height="23"
                     Width="50" />
            <TextBox x:Name="centery"
                     HorizontalAlignment="Left"
                     Text="0"
                     Margin="167,29,0,0"
                     VerticalAlignment="Top"
                     Foreground="Black"
                     FontWeight="Bold"
                     Height="24"
                     Width="50" />
            <TextBox x:Name="centerz"
                     Text="50"
                     HorizontalAlignment="Right"
                     Margin="0,29,42,0"
                     VerticalAlignment="Top"
                     Foreground="Black"
                     FontWeight="Bold"
                     Height="23"
                     Width="50" />
            <Label x:Name="ob_xrot"
                   Content="Length:X"
                   HorizontalAlignment="Left"
                   FontWeight="Bold"
                   Margin="-1,89,0,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontSize="10"
                   Height="23"
                   Width="60" />
            <Label x:Name="ob_yrot"
                   Content="Y"
                   HorizontalAlignment="Left"
                   Margin="146,89,0,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontWeight="Bold"
                   FontSize="10"
                   Height="24"
                   Width="30" />
            <Label x:Name="ob_zrot"
                   Content="Z"
                   HorizontalAlignment="Right"
                   Margin="0,89,89,0"
                   VerticalAlignment="Top"
                   Foreground="Black"
                   FontWeight="Bold"
                   FontSize="10"
                   Height="23"
                   Width="19" />
            <TextBox x:Name="LengthX"
                     Text="100"
                     HorizontalAlignment="Left"
                     FontWeight="Bold"
                     Margin="52,89,0,0"
                     VerticalAlignment="Top"
                     Foreground="Black"
                     Height="23"
                     Width="50" />
            <TextBox x:Name="LengthY"
                     HorizontalAlignment="Left"
                     Text="40"
                     Margin="165,89,0,0"
                     VerticalAlignment="Top"
                     Foreground="Black"
                     FontWeight="Bold"
                     Height="24"
                     Width="50" />
            <TextBox x:Name="LengthZ"
                     HorizontalAlignment="Right"
                     Text="100"
                     Margin="0,89,37,0"
                     VerticalAlignment="Top"
                     Foreground="Black"
                     FontWeight="Bold"
                     Height="23"
                     Width="50" />
            <Button x:Name="en"
                    Content="确定"
                    HorizontalAlignment="Left"
                    Margin="93,221,0,0"
                    VerticalAlignment="Top"
                    Width="50"
                    Height="28"
                    FontSize="11"
                    Click="en_Click" />
            <Button x:Name="not"
                    Content="取消"
                    Margin="216,221,134,0"
                    VerticalAlignment="Top"
                    Width="50"
                    Height="28"
                    FontSize="11"
                    Click="not_Click" />
        </Grid>
    </Viewbox>
</Window>
